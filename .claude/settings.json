{
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "enabledMcpjsonServers": ["context7"],
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": true,
    "excludedCommands": ["git"],
    "network": {
      "allowUnixSockets": [""]
    }
  },
  "permissions": {
    "allow": [
    ],
    "deny": [
      "Read(//**/secrets/**)",
      "Read(//**/.env)",
      "Read(//**/.env.*)",
      "Read(//**/.ssh/**)",
      "Read(//**/.aws/**)",
      "Read(//**/.config/gcloud/**)",
      "Read(//**/credentials.json)",
      "Read(//**/id_rsa)",
      "Read(//**/id_ed25519)"
    ],
    "ask": [
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git reset:*)",
      "Bash(sudo rm:*)",
      "Bash(sudo chown:*)",
      "Bash(chmod:*)",
      "Bash(python3:*)",
      "Bash(curl:*)",
      "Bash(sed:*)",
      "Bash(mv:*)",
      "Bash(cp:*)",
      "Bash(xargs sed:*)"
    ]
  }
}
